<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	  package="com.cansiny.eform"
	  android:versionCode="10001"
	  android:versionName="ver1.1-build0001" android:installLocation="auto">

  <uses-sdk
      android:minSdkVersion="13"
      android:targetSdkVersion="17" />

  <application
      android:name="EFormApplication"
      android:logo="@drawable/launcher"
      android:icon="@drawable/launcher"
      android:label="@string/app_name"
      android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"
      android:allowBackup="true"
      android:allowClearUserData="false"
      android:largeHeap="true" android:hardwareAccelerated="true">

    <activity
        android:name="com.cansiny.eform.HomeActivity"
        android:configChanges="orientation|keyboardHidden|screenSize"
        android:label="@string/app_name"
        android:singleUser="true"
	android:hardwareAccelerated="true">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />	
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <activity android:name="com.cansiny.eform.LogActivity">
    </activity>

    <activity android:name="com.cansiny.eform.FormActivity">
    </activity>

    <provider android:name="com.cansiny.eform.EFormContentProvider"
        android:authorities="com.cansiny.eform.EFormContents"
        android:label="@string/app_contents_name"
        android:multiprocess="false"
	android:enabled="true"
	android:syncable="true"
	android:exported="true"
	android:singleUser="true">
    </provider>

  </application>

</manifest>
